{
  "name": "@solidbasisventures/intelliwaketsnode",
  "version": "2.0.302",
  "description": "NPM package for IntelliWake Node Components",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "BuildAndPublish": "tsc && vite build",
    "OLD_BuildAndPublish": "rollup -c && npm version patch --no-git-tag-version && npm publish",
    "YarnInstall": "yarn install && yarn add -D @solidbasisventures/intelliwaketsfoundation --force -W",
    "YarnUpgrade": "yarn upgrade && yarn add -D @solidbasisventures/intelliwaketsfoundation --force -W",
    "Test": "jest",
    "TSNode": "ts-node --script-mode consoles/consoles.ts",
    "TSNodeDev": "ts-node-dev --script-mode --respawn --transpile-only consoles/consoles.ts"
  },
  "files": [
    "dist"
  ],
  "author": "SolidBasisVentures",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SolidBasisVentures/IntelliWakeTSNode.git"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@solidbasisventures/intelliwaketsfoundation": "^5.2.34",
    "@types/mssql": "^6.0.2",
    "@types/mysql": "^2.15.15",
    "@types/node": "^16.11.11",
    "@types/pg": "^8.6.5",
    "mssql": "^6.2.0",
    "mysql": "^2.18.1",
    "pg": "^8.7.3",
    "pg-cursor": "^2.7.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
	"vite": "^4.1.1",
	"vitest": "^0.28.5",
	"vite-plugin-dts": "^1.7.2",
	"rollup-plugin-node-polyfills": "^0.2.1"
  }
}
