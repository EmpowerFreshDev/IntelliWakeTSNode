{
	"name": "@solidbasisventures/intelliwaketsnode",
	"version": "2.1.5",
	"description": "NPM package for IntelliWake Node Components",
	"type": "commonjs",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.umd.cjs"
		}
	},
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"scripts": {
		"Vitest-Watch": "vitest",
		"Publish": "tsc && vite build && pnpm version patch && git push && npm publish",
		"Install-IntelliWake": "pnpm install && pnpm update @solidbasisventures/intelliwaketsfoundation@latest",
		"TSNodeDev": "ts-node-dev --script-mode --respawn --transpile-only consoles/consoles.ts",
		"Build": "tsc && vite build",
		"Version-Minor-Advance": "pnpm version minor"
	},
	"files": [
		"dist"
	],
	"author": "SolidBasisVentures",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SolidBasisVentures/IntelliWakeTSNode.git"
	},
	"dependencies": {},
	"devDependencies": {
		"@solidbasisventures/intelliwaketsfoundation": "^5.3.6",
		"@types/mssql": "^6.0.2",
		"@types/mysql": "^2.15.15",
		"@types/node": "^16.11.11",
		"@types/pg": "^8.6.5",
		"@types/split2": "^3.2.1",
		"split2": "^4.1.0",
		"mssql": "^6.2.0",
		"mysql": "^2.18.1",
		"pg": "^8.7.3",
		"pg-cursor": "^2.7.3",
		"prettier": "^2.7.1",
		"source-map-support": "^0.5.19",
		"ts-node": "^10.8.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.7.4",
		"vite": "^4.1.1",
		"vite-plugin-dts": "^1.7.2",
		"vitest": "^0.28.5"
	}
}
